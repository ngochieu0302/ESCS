<script>
    $(document).on('focusout', 'input.number[type=text]', function () {
        var $input = $(this);
        if ($input.val() == "") {
            $input.val("0");
        }
    }).on('focus', 'input.number[type=text]', function () {
        var $input = $(this);
        if ($input.val() == "0") {
            $input.val("");
        }
    });

    $(document).on('focusout', 'input.decimal[type=text]', function () {
        var $input = $(this);
        if ($input.val() == "") {
            $input.val("0");
        }
    }).on('focus', 'input.decimal[type=text]', function () {
        var $input = $(this);
        if ($input.val() == "0") {
            $input.val("");
        }
    });

    $(".custom-modal .modal").draggable().resizable();

    $(".custom-modal").on('shown.bs.modal', function () {
        $("body").removeClass("modal-open");
        $(".custom-modal").removeClass("modal");
    });

    navigator.permissions.query({ name: 'microphone' })
        .then((permissionObj) => {
            console.log("Trạng thái quyền microphone: ", permissionObj.state);
        })
        .catch((error) => {
            console.log("Trạng thái quyền microphone: ", error);
        });

    navigator.permissions.query({ name: 'camera' })
        .then((permissionObj) => {
            console.log("Trạng thái quyền camera: ", permissionObj.state);
        })
        .catch((error) => {
            console.log("Trạng thái quyền camera: ", error);
        });

    var _stringeeService = new StringeeService();
    _stringeeService.connect();
</script>