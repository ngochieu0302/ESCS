@* Danh sách gcn tìm kiếm *@
<script type="text/html" id="modalOtherSearchDsGCNTemplate">
    <% if(data_info.length > 0){
    _.forEach(data_info, function(item,index) { %>
    <tr style="cursor: pointer" data_ma_chi_nhanh="<%- item.ma_chi_nhanh %>" data_so_id="<%- item.so_id_hdong %>" data_so_id_dt="<%- item.so_id_gcn %>" data_nv="<%- item.nv %>" class="tkiem_kh" id="tkiem_kh_<%- item.so_id_hdong %>_<%- item.so_id_gcn %>" onclick="onChonKhachHang('<%- item.so_id_hdong %>','<%- item.so_id_gcn %>')">
        <td class="text-center"><%- item.so_hdong %></td>
        <td class="text-center"><%- item.ten_doi_tuong %></td>
        <td class="text-center">
            <a href="#" onclick="xemThongTinGiayChungNhan('<%- item.ma_chi_nhanh %>', '<%- item.so_id_hdong %>', '<%- item.so_id_gcn %>')"><%- item.so_gcn %></a>
        </td>
        <td class="text-center">
            <%- item.nv_text %>
        </td>
        <td class="text-center"><%- item.ngay_hl_bh %> - <%- item.ngay_kt_bh %></td>
        <td><%- item.phan_nhom_text %></td>
        <td><%- item.ten_chi_nhanh %></td>
    </tr>
    <%})}%>
    <% if(data_info.length < 4){
    for(var i = 0; i < 4 - data_info.length;i++ ){ %>
    <tr style="cursor: pointer">
        <td style="height:34.6px"></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
        <td></td>
    </tr>
    <%}}%>
</script>

@*Xác minh phí khi lấy số hồ sơ*@
<script type="text/html" id="dsXacMinhPhiLaySoHS_template">
    <% if(danh_sach.length > 0){
    _.forEach(danh_sach, function(item,index) { %>
    <tr>
        <td class="text-right">
            <%-ESUtil.formatMoney(item.so_tien) %>
        </td>
        <td class="text-right">
            <%- ESUtil.formatMoney(item.so_tien_da_tt) %>
        </td>
        <td class="<%- item.style %>"><%- item.ghi_chu %></td>
    </tr>
    <% })}else{ %>
    <tr>
        <td class="text-center" colspan="3">
            <p class="m-0">Chưa có dữ liệu hiển thị</p>
        </td>
    </tr>
    <% } %>
</script>