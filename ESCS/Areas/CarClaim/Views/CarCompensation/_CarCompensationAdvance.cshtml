<div id="CarCompensationAdvance" class="modal fade" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog" aria-hidden="true" style="z-index: 1600">
    <div id="modalDialogThemTamUng" class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header py-1">
                <h4 class="modal-title">Thêm yêu cầu tạm ứng</h4>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body">
                <form name="frmCarCompensationAdvance" method="post" id="frmCarCompensationAdvance">
                    <input type="hidden" name="so_id" />
                    <input type="hidden" name="so_id_tu" />
                    <input type="hidden" name="ma_doi_tac_ql" />
                    <input type="hidden" name="ma_cnhanh_ql" />
                    <div class="row">
                        <div class="col-3">
                            <div class="form-group">
                                <label class="_required">Phương thức TT</label>
                                <select class="select select2 form-control custom-select" required name="pttt" style="width:100%">
                                    <option value="">Chọn phương thức</option>
                                    <option value="CK">Chuyển khoản</option>
                                    <option value="TM">Tiền mặt</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-4">
                            <div class="form-group">
                                <label class="_required">Ngân hàng thụ hưởng</label>
                                <select class="select select2 form-control custom-select" required name="ma_nh" style="width:100%">
                                </select>
                            </div>
                        </div>
                        <div class="col-5">
                            <div class="form-group">
                                <label class="_required">Chi nhánh thụ hưởng</label>
                                <select class="select select2 form-control custom-select" required name="ma_cnhanh_nh" style="width:100%">
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-top:8px">
                        <div class="col-3">
                            <div class="form-group">
                                <label for="ngay_d">Ngày tạm ứng</label>
                                <div class="input-group">
                                    <input type="text" class="form-control datepicker" autocomplete="off" name="ngay_yc" display-format="date" value-format="number" placeholder="dd/mm/yyyy">
                                    <div class="input-group-append">
                                        <span class="input-group-text" style="padding: 0.1em 0.3em !important;"><span class="ti-calendar"></span></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-3">
                            <div class="form-group">
                                <label class="_required">Số tài khoản/cmt thụ hưởng</label>
                                <input type="text" class="form-control" maxlength="20" autocomplete="off" placeholder="Số tài khoản thụ hưởng" required name="tk_cmt">
                            </div>
                        </div>
                        <div class="col-3">
                            <div class="form-group">
                                <label class="_required">Tên đối tượng thụ hưởng</label>
                                <input type="text" class="form-control" maxlength="100" autocomplete="off" placeholder="Tên đối tượng thụ hưởng" required name="ten">
                            </div>
                        </div>
                        <div class="col-3">
                            <div class="form-group">
                                <label class="_required">Số tiền</label>
                                <input type="text" class="form-control number" autocomplete="off" placeholder="Số tiền" required name="tien">
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-top:8px">
                        <div class="col-6">
                            <div class="form-group">
                                <label class="_required">Địa chỉ nhận</label>
                                <input type="text" class="form-control" maxlength="500" autocomplete="off" placeholder="Địa chỉ nhận" required name="dia_chi_nhan">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-group">
                                <label class="_required">Nội dung</label>
                                <input type="text" class="form-control" maxlength="500" autocomplete="off" placeholder="Nội dung" required name="nd">
                            </div>
                        </div>
                    </div>
                    <div class="row" style="margin-top:8px">
                        <div class="col-12">
                            <div class="form-group">
                                <label class="_required">Ghi chú</label>
                                <textarea class="form-control" maxlength="500" rows="10" autocomplete="off" placeholder="Ghi chú, lý do tạm ứng" required name="ghi_chu"></textarea>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                @*<button type="button" class="btn btn-primary btn-sm wd-80 ml-2" id="btnCarCompensationAdvance_save">
                    <i class="fas fa-save mr-2"></i>Lưu
                </button>
                <button type="button" class="btn btn-primary btn-sm ml-2" id="btnCarCompensationAdvance_save_close">
                    <i class="fas fa-save mr-2"></i>Lưu và đóng
                </button>*@
                <button type="button" class="btn btn-primary btn-sm ml-2" id="btnCarCompensationAdvance_processing">
                    <i class="fas fa-eye mr-2"></i>Tiến trình xử lý
                </button>
                <button class="btn btn-primary btn-sm wd-85 mg-t-22" data-dismiss="modal"><i class="fas fa-window-close"></i>&nbsp;&nbsp;Đóng</button>
            </div>
        </div>
    </div>

    <div class="modal-dialog" id="modalDialogQTXL" style="width:400px;float:left;margin:5px;display:none">
        <div class="modal-content">
            <div class="modal-header py-1">
                <h5 class="modal-title">Quá trình xử lý</h5>
                <button type="button" class="close" id="btnCloseQTXL" aria-hidden="true">×</button>
            </div>
            <div class="modal-body p-0">
                <div class="timeline mt-3" id="tabQuaTrinhGiaiQuyetTimeLine" style="height:457px; overflow:auto">

                </div>
            </div>
        </div>
    </div>
</div>