                        <div class="col-sm-3">
                            <label for="formControlRange">File upload</label>
                            <div class="input-group">
                                <div class="custom-file">
                                    <input type="file" name="inputFile" class="custom-file-input" id="inputGroupFile01">
                                    <label class="custom-file-label" for="inputGroupFile01">Choose file</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label>Select2 (Tag)</label>
                                <select class="select2 block form-control custom-select" multiple="multiple" style="width: 100%; height:36px;">
                                    <option>Select</option>
                                    <optgroup label="Alaskan/Hawaiian Time Zone">
                                        <option value="AK" selected>Alaska</option>
                                        <option value="HI">Hawaii</option>
                                    </optgroup>
                                    <optgroup label="Pacific Time Zone">
