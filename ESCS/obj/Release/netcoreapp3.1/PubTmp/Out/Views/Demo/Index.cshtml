                            <div class="form-group">
                                <label for="disabledTextInput">View only</label>
                                <p class="form-control-static"> John </p>
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label>Default</label>
                                <input type="text" class="form-control" value="Escs...">
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label>Number only</label>
                                <input type="text" class="form-control number">
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label>Decimal</label>
                                <input type="text" class="form-control decimal">
                            </div>
                        </div>
                        <div class="col-sm-3">
                            <div class="form-group">
                                <label>Input with tooltip</label>
