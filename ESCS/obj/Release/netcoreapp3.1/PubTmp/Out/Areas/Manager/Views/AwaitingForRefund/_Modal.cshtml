                            <button type="button" class="btn btn-primary btn-sm wd-140 float-right mr-2" id="btnThemHoSoTon">
                                <i class="fas fa-plus mr-2"></i>Thêm hồ sơ
                            </button>
                            <button type="button" class="btn btn-primary btn-sm wd-140 float-right mr-2" id="btnLuuDeNghiHoanQuyHoSo">
