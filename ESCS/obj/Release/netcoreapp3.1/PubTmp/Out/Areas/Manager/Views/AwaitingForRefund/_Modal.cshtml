                                        <option value="PHIDV">Phí dịch vụ</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-2">
                                <div class="form-group">
                                    <label class="">Đối tác quản lý</label>
                                    <select class="select2 form-control custom-select" name="ma_doi_tac_ql" style="width: 100%; height:36px;" required>
                                    </select>
                                </div>
                            </div>
