                                    <label for="ngay_d">Từ ngày</label>
                                    <div class="input-group" style="width: 105px;">
                                        <input type="text" autocomplete="off" class="form-control datepicker" display-format="date" value-format="number" name="ngay_d" placeholder="mm/dd/yyyy">
                                        <div class="input-group-append">
                                            <span class="input-group-text px-2"><span class="ti-calendar"></span></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-1 px-0">
                                <div class="form-group">
                                    <label for="ngay_c">Đến ngày</label>
                                    <div class="input-group" style="width: 105px;">
                                        <input type="text" autocomplete="off" class="form-control datepicker" display-format="date" value-format="number" name="ngay_c" placeholder="mm/dd/yyyy">
                                        <div class="input-group-append">
                                            <span class="input-group-text px-2"><span class="ti-calendar"></span></span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-3 table-responsive">
                                <div class="form-group">
                                    <label class="">Số hồ sơ</label>
                                    <div class="input-group">
                                        <input type="text" autocomplete="off" name="so_hs" class="form-control" placeholder="Số hồ sơ hoàn quỹ tồn">
                                        <div class="input-group-append">
                                            <label class="input-group-text" for="so_hs">
                                                <a href="#" id="btnTimKiemHoSoTon"><i class="fas fa-search" title="Số hồ sơ"></i></a>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                    <div class="row">
                        <div class="col-12 table-responsive mt-1">
                            <div class="table-app">
                                <div class="table-responsive" style="height:340px">
                                    <table class="table table-bordered fixed-header" style="width:100%">
                                        <thead class="font-weight-bold">
                                            <tr class="text-center uppercase">
                                                <th style="width:46px">
                                                    <div class="custom-control custom-checkbox ml-2">
                                                        <input type="checkbox" onchange="onChonTatCa(this)" id="ttbt_chon_tat_ca" class="custom-control-input">
