@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Thu đòi người thứ 3";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row page-titles">
    <div class="col-md-5 col-12 align-self-center">
        <h3 class="text-themecolor mb-0">@ViewData["Title"]</h3>
    </div>
    <div class="col-md-7 col-12 align-self-center justify-content-end d-none d-md-flex">
        <ol class="breadcrumb mb-0">
            <li class="breadcrumb-item">
                <a href="javascript:void(0)">Trang chủ</a>
            </li>
            <li class="breadcrumb-item active">@ViewData["Title"]</li>
        </ol>
    </div>
</div>

<div class="container-fluid">
    <div class="row">
        <div class="col-sm-12">
            <div class="card card-body" style="padding-top:5px;">
                <form action="/" method="post" name="frmTimKiem" novalidate="novalidate">
                    <div class="row">
                        <div class="col col-2">
                            <div class="form-group">
                                <label for="ngay_d">Tìm kiếm từ ngày</label>
                                <div class="input-group">
                                    <input type="text" autocomplete="off" class="form-control datepicker" name="ngay_d" display-format="date" value-format="number" placeholder="dd/mm/yyyy">
                                    <div class="input-group-append">
                                        <span class="input-group-text"><span class="ti-calendar"></span></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col col-2">
                            <div class="form-group">
                                <label for="ngay_c">Đến ngày</label>
                                <div class="input-group">
                                    <input type="text" autocomplete="off" class="form-control datepicker" name="ngay_c" display-format="date" value-format="number" placeholder="dd/mm/yyyy">
                                    <div class="input-group-append">
                                        <span class="input-group-text"><span class="ti-calendar"></span></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col col-2">
                            <div class="form-group">
                                <label for="ma_chi_nhanh">Đơn vị</label>
                                <select class="select2 form-control custom-select" name="ma_chi_nhanh" style="width: 100%; height:36px;">
                                </select>
                            </div>
                        </div>
                        <div class="col col-2">
                            <div class="form-group">
                                <label for="so_hs">Số hồ sơ</label>
                                <input type="text" class="form-control" autocomplete="off" name="so_hs" placeholder="Số hồ sơ">
                            </div>
                        </div>
                        <div class="col col-2">
                            <div class="form-group">
                                <label for="so_hs">Tên người thu đòi</label>
                                <input type="text" class="form-control" autocomplete="off" name="ten" placeholder="Tên người thu đòi thứ 3">
                            </div>
                        </div>
                        <div class="col col-2" style="margin-top:1.3rem">
                            <button type="button" class="btn btn-primary btn-sm wd-40p mr-2" id="btnFrmRetrieve3rdSearch" title="Tìm kiếm">
                                <i class="fa fa-search"></i>
                            </button>
                            <button type="button" class="btn btn-primary btn-sm wd-40p" id="btnNewRetrieve3rd" title="Thêm mới hồ sơ">
                                <i class="fa fa-plus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="row">


                    </div>
                </form>
                <div class="row" style="margin-top: 3px;">
                    <div class="col-md-12">
                        <div class="table-responsive">
                            <div id="gridViewThuDoiNguoiThu3" class="table-app" style="height: 64vh;"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<partial name="_Modal.cshtml" />
<partial name="_Template.cshtml" />
<partial name="/Views/Shared/_FwdApproval.cshtml" />
@section scripts{
    <script src="~/js/app/Admin/services/PartnerListService.js" asp-append-version="true"></script>
    <script src="~/js/app/Admin/services/BranchListService.js" asp-append-version="true"></script>
    <script src="~/js/app/ModalTrinhDuyetService.js" asp-append-version="true"></script>
    <script src="~/js/app/Manager/services/Retrieve3rdService.js" asp-append-version="true"></script>
    <script src="~/js/app/Manager/Retrieve3rd.js" asp-append-version="true"></script>
}