<div id="modalXemThongTinChungNhan" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true" style="z-index: 1600;">
    <div class="modal-dialog modal-lg" style="max-width:60%;">
        <div class="modal-content">
            <div class="modal-header py-1">
                <h4 class="modal-title">Thông tin chứng nhận bảo hiểm</h4>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body" style="max-height: 75vh; overflow: auto">
                <div class="row">
                    <div class="col col-12">
                        <table class="table" id="tblHealthCommonCertificate">
                            <tr>
                                <td>Tên KH</td>
                                <td data-model="ten_kh"></td>
                                <td>Tên người tham gia</td>
                                <td data-model="ten_nguoi_tham_gia"></td>
                            </tr>
                            <tr>
                                <td>Ngày sinh</td>
                                <td data-model="ngay_sinh"></td>
                                <td>Giới tính</td>
                                <td data-model="gioi_tinh"></td>
                            </tr>
                            <tr>
                                <td>Số điện thoại</td>
                                <td data-model="d_thoai"></td>
                                <td>Số CMT</td>
                                <td data-model="so_cmt"></td>
                            </tr>
                            <tr>
                                <td>Email</td>
                                <td data-model="email"></td>
                                <td>Địa chỉ</td>
                                <td data-model="dchi"></td>
                            </tr>
                            <tr>
                                <td>Số GCN</td>
                                <td data-model="gcn"></td>
                                <td>Sản phẩm</td>
                                <td data-model="lh_nv"></td>
                            </tr>
                            <tr>
                                <td>Gói bảo hiểm</td>
                                <td data-model="ten_goi_bh"></td>
                                <td>Đơn vị công tác</td>
                                <td><span data-model="dvi_ctac"></span>
                            </tr>
                            <tr>
                                <td>CN công tác</td>
                                <td data-model="cnhanh_ctac"></td>
                                <td>Mã nhân viên</td>
                                <td data-model="manv_ctac"></td>
                            </tr>
                            <tr>
                                <td>Chức vụ</td>
                                <td data-model="cvu_ctac"></td>
                                <td>Phòng ban</td>
                                <td data-model="pban_ctac"></td>
                            </tr>
                            <tr>
                                <td>Hiệu lực BH</td>
                                <td data-model="hieu_luc"></td>
                                <td></td>
                                <td></td>
                            </tr>
                        </table>
                    </div>
                </div>
                <div class="row mt-2">
                    <div class="col col-12">
                        <h6>Tình trạng thanh toán phí</h6>
                        <div class="table-responsive" style="max-height:155px">
                            <table id="tableDSXacMinhPhiLaySoHS" class="table table-bordered fixed-header" style="width:100%">
                                <thead class="font-weight-bold">
                                    <tr class="text-center uppercase">
                                        <th style="width:170px">Tổng phí theo hợp đồng</th>
                                        <th style="width:150px">Số tiền đã nộp</th>
                                        <th>Ghi chú</th>
                                    </tr>
                                </thead>
                                <tbody id="dsXacMinhPhiLaySoHS"></tbody>
                            </table>
                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer" style="display:block">
                <button class="btn btn-primary btn-sm wd-85 float-right" data-dismiss="modal" id="CarCommonCertificate_close"><i class="fas fa-window-close mr-2"></i>Đóng</button>
                @*<button type="button" class="btn btn-primary btn-sm" id="btnSuaGCN" title="Sửa thông tin giấy chứng nhận"><i class="fa fa-edit"></i> Sửa thông tin đối tượng BH</button>*@
            </div>
        </div>
    </div>
</div>

<script type="text/html" id="dsXacMinhPhiLaySoHS_template">
    <% if(danh_sach.length > 0){
    _.forEach(danh_sach, function(item,index) { %>
    <tr>
        <td class="text-right">
            <%- ESUtil.formatMoney(item.so_tien) %>
        </td>
        <td class="text-right">
            <%- ESUtil.formatMoney(item.so_tien_da_tt) %>
        </td>
        <td class="<%- item.style %>"><%- item.ghi_chu %></td>
    </tr>
    <% })}else{ %>
    <tr>
        <td class="text-center" colspan="3">
            <p class="m-0">Chưa có dữ liệu hiển thị</p>
        </td>
    </tr>
    <% } %>
</script>